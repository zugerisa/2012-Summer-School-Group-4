\begin_layout Plain Layout

templateGray = double(templateGray);
\end_layout

\begin_layout Plain Layout

[templateHeight,templateWidth] = size(templateGray);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

%% 2.
 correlation calculation 
\end_layout

\begin_layout Plain Layout

frameMean = conv2(frameGray,ones(size(templateGray))./numel(templateGray),'same')
;
\end_layout

\begin_layout Plain Layout

templateMean = mean(templateGray(:)); 
\end_layout

\begin_layout Plain Layout

corrPartI = conv2(frameGray,fliplr(flipud(templateGray-templateMean)),'same')./nu
mel(templateGray);
\end_layout

\begin_layout Plain Layout

corrPartII = frameMean.*sum(templateGray(:)-templateMean);
\end_layout

\begin_layout Plain Layout

stdFrame = sqrt(conv2(frameGray.^2,ones(size(templateGray))./numel(templateGray),'
same')-frameMean.^2);
\end_layout

\begin_layout Plain Layout

stdTemplate = std(templateGray(:));
\end_layout

\begin_layout Plain Layout

corrScore = (corrPartI-corrPartII)./(stdFrame.*stdTemplate);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

%% 3.
 finding most likely region 
\end_layout

\begin_layout Plain Layout

[maxVal,maxIdx] = max(corrScore(:)); 
\end_layout

\begin_layout Plain Layout

[maxR, maxC] = ind2sub([size(corrScore,1),size(corrScore,2)],maxIdx);
\end_layout

\begin_layout Plain Layout

\end_layout

\begin_layout Plain Layout

%% 4.
 hypothesis test 
\end_layout

\begin_layout Plain Layout

if ~exist('threshC','var')     
\end_layout

\begin_layout Plain Layout

	threshC = .75;
\end_layout

\begin_layout Plain Layout

end 
\end_layout

\begin_layout Plain Layout

if maxVal>=threshC     
\end_layout

\begin_layout Plain Layout

	boundingBox(1,:) = [max(1,maxR-round(templateHeight/2)), max(1,maxC-round(templ
ateWidth/2)), templateHeight, templateWidth]; 
\end_layout

\begin_layout Plain Layout

else     
\end_layout

\begin_layout Plain Layout

	boundingBox(1,:) = []; 
\end_layout

\begin_layout Plain Layout

end
\end_layout

